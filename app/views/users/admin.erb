<table class="table">
  <thead>
    <tr>
      <th scope="col">이메일</th>
      <th scope="col">패스워드</th>
      <th scope="col">닉네임</th>
      <th scope="col">비고</th>
    </tr>
  </thead>
  <tbody>
    <% @users.reverse.each do |u|%>
    <tr>
      <th scope="row"><%= u.name%></th>
      <th><%= u.password%></th>
      <td><%= u.nicname%></td>
      <td>
          <a href=/users/edit/<%= u.id%>><button type="button" class="btn btn-outline-success">수정</button></a>
          <a href=/users/delete/<%= u.id%>><button type="button" class="btn btn-outline-danger">삭제</button></a>
      </td>
          
    </tr>
    <% end %>
  </tbody>
</table>